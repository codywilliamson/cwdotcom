{
    "name": "codywilliamson",
    "version": "3.0.0",
    "description": "Personal site for software engineer, Cody Williamson.",
    "main": "index.js",
    "scripts": {
        "watch:eleventy": "eleventy --serve",
        "build:eleventy": "eleventy",
        "build:sass": "sass --style=compressed src/assets/sass:./public/assets/css",
        "watch:sass": "sass --watch src/assets/sass:./public/assets/css",
        "watch:cms": "npx netlify-cms-proxy-server",
        "start": "del-cli ./public --force && npm-run-all --parallel watch:*",
        "build": "npm-run-all --parallel build:*"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/codywilliamson/cwdotcom"
    },
    "keywords": [
        "software",
        "developer",
        "engineer",
        "cloud",
        "csharp",
        "portfolio"
    ],
    "author": "Cody Williamson",
    "license": "TBD",
    "bugs": {
        "url": "https://github.com/codywilliamson/cwdotcom/issues"
    },
    "homepage": "https://github.com/codywilliamson/cwdotcom-#readme",
    "devDependencies": {
        "@11ty/eleventy": "^2.0.1",
        "@11ty/eleventy-navigation": "^0.3.5"
    },
    "dependencies": {
        "bulma": "^0.9.4",
        "del-cli": "^5.0.0",
        "html-minifier": "^4.0.0",
        "netlify-cms-proxy-server": "^1.3.24",
        "npm-run-all": "^4.1.5",
        "sass": "^1.63.3"
    }
}
